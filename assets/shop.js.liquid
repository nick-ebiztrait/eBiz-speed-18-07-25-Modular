//el.siblings('.inputCounter').val(value);
    self.inputCounter(el, value);
  })
  .on('click', '.inputCounter-up', function () {
    
    var el = $(this),
        $input = el.siblings('.inputCounter'),
        value = parseInt($input.val()) + 1;
//el.siblings('.inputCounter').val(value);
    self.inputCounter(el, value);
  });

      

		/* Remove line item on x click */
