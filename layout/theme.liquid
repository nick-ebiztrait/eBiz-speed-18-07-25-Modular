    <meta charset="utf-8">
    <title>
      {{ page_title }}
      {% if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif %}
      {% if current_page != 1 %} &ndash; Page {{ current_page }}{% endif %}
      {% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
    </title>

    {%- if settings.favicon != blank -%}
      <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
    {%- endif -%}

   

    <!--[if IE]> <meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1' /> <![endif]-->
    <link rel="canonical" href="{{ canonical_url }}">
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no"
    >
    <link rel="preload" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.7.1/css/all.min.css" as="style">
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.7.1/css/all.min.css"
      integrity="sha512-5Hs3dF2AEPkpNAR7UiOHba+lRSJNeM2ECkwxUIxC1Q/FLycGTbNapWXB4tP889k5T5Ju8fs4b1P5z/iB4nMfSQ=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    >
   

    {{- 'theme.scss.css' | asset_url | stylesheet_tag -}}
    {{- 'new.css' | asset_url | stylesheet_tag -}}

    {{ content_for_header }}

    {%- if settings.enable_yotpo and settings.yotpo_ID != '' -%}
      {%- include 'yotpo-script' -%}
    {%- endif -%}
    {% include 'wiser' %}




  </head>

  {%- capture body_class -%}{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }} image-hover-light cta-fill-black{% if settings.transition_images_fade %} enable-fade-in{% endif %} no-outline{%- endcapture -%}

  <body id="{{ page_title | handle }}" class="{{ body_class }}">
    <a class="in-page-link skip-link" href="#MainContent">{{ 'general.accessibility.skip_to_content' | t }}</a>
    {%- section 'popup' -%}

    <div class="pageWrap">
      {%- section 'header' -%}

      <div class="bodyWrap">
        <main class="main-content js-focus-hidden" id="MainContent" role="main" tabindex="-1">
          {{ content_for_layout }}
        </main>

        {%- section 'footer' -%}
      </div>
      <!-- END bodyWrap -->
    </div>
    <!-- END pageWrap -->

    <!-- AJAX Mustache Templates -->
    {%- include 'ajax-cart' -%}

    {%- include 'scripts' -%}

    {%- include 'load-spinner' -%}
    {{ '//cdn.shopify.com/s/files/1/0194/1736/6592/t/1/assets/booster-page-speed-optimizer.js?23' | script_tag }}

    <script>
      $('.footer__block.footer__block--menus ul li.footer__block-menu-link:last-child a').attr('target', '_blank');
    </script>
 {% render 'size-chart-default' %}
 <link rel="preload" href="{{ 'splide.min.js' | asset_url }}" as="script">
<script src="{{ 'splide.min.js' | asset_url }}"></script>

  </body>
</html>
