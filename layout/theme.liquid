        "url": "https://estellboutiques.com/",
        "potentialAction": {
          "@type": "SearchAction",
          "target": "https://estellboutiques.com/search?q{search_term_string}",
          "query-input": "required name=search_term_string"
        }
      }
    </script>

    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@type": "Organization",
